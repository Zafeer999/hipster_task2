{
    "name": "zafeer/laravel-discounts",
    "description": "A reusable user-level discount management package for Laravel",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Zafeer\\Discounts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zafeer\\Discounts\\Tests\\": "tests/",
            "Database\\Factories\\": "database/factories/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zafeer\\Discounts\\DiscountServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}